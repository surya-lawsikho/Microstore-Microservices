# AWS API Gateway Environment Variables
# Copy this file to .env and update with your values

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# Microservices URLs (for local development)
USERS_URL=http://localhost:3001
PRODUCTS_URL=http://localhost:3002
ORDERS_URL=http://localhost:3003

# For production, these would be your deployed microservice URLs
# USERS_URL=https://your-users-service.aws.com
# PRODUCTS_URL=https://your-products-service.aws.com
# ORDERS_URL=https://your-orders-service.aws.com

# Serverless Framework Configuration
STAGE=dev
REGION=us-east-1
